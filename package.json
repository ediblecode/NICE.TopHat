{
  "name": "NICE.TopHat",
  "version": "0.1.4",
  "description": "Distributable, branded tophat component for NICE Services and Web Applications",
  "private": true,
  "scripts": {
    "start": "grunt parallel --color",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "grunt mochaTest:unit --color",
    "test:unit:teamcity": "grunt mochaTest:teamcity --color",
    "test:functional": "node ./node_modules/webdriverio/bin/wdio",
    "test:functional:keyboardNav": "node ./node_modules/webdriverio/bin/wdio --spec ./test/specs/functional/keyboardNav.js",
    "test:visual": "./node_modules/webdriverio/bin/wdio --spec ./test/specs/visual.js",
    "test:browserstack": "node ./node_modules/webdriverio/bin/wdio wdio.conf.browserstack.js",
    "lint": "grunt eslint:default --color",
    "lint:teamcity": "grunt eslint:teamcity --color",
    "build": "grunt build --color"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nhsevidence/NICE.Tophat.git"
  },
  "author": "Matt Smith <mattonfoot@gmail.com> (http://about.me/mattonfoot)",
  "license": "MIT",
  "bugs": {
    "url": "http://imt-mingle.nice.org.uk/projects/tophat/overview"
  },
  "homepage": "http://github.com/nhsevidence/NICE.Tophat",
  "devDependencies": {
    "@nice-digital/eslint-config": "^1.0.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "browserify": "14.4.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "cssify": "^1.0.3",
    "eslint-teamcity": "^1.4.0",
    "glob": "^7.1.1",
    "grunt": "^1.0.1",
    "grunt-blanket": "0.0.10",
    "grunt-browserify": "^5.0.0",
    "grunt-casperjs": "^2.2.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-htmlmin": "^2.2.0",
    "grunt-contrib-less": "^1.4.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^20.1.0",
    "grunt-mocha-test": "^0.13.2",
    "grunt-parallel": "^0.5.1",
    "js-cookie": "^2.1.3",
    "load-grunt-config": "^0.19.2",
    "load-grunt-tasks": "^3.5.2",
    "minifyify": "^7.3.4",
    "mocha": "^3.5.0",
    "mocha-teamcity-reporter": "^1.1.1",
    "node-lessify": "^0.1.4",
    "partialify": "^3.1.6",
    "sinon": "^3.1.0",
    "wdio-browserstack-service": "^0.1.12",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-screenshot": "^0.2.0",
    "wdio-spec-reporter": "^0.1.0",
    "wdio-teamcity-reporter": "^1.1.1",
    "wdio-visual-regression-service": "^0.3.0",
    "webdriverio": "^4.8.0"
  }
}
